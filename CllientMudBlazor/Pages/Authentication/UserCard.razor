<MudCard Elevation="0" Square="true" Class="@Class">
    <MudCardHeader>
        <CardHeaderAvatar>
            <MudAvatar>@LetterOfName</MudAvatar>
        </CardHeaderAvatar>
        <CardHeaderContent>
            <MudStack>
                <MudText Typo="Typo.body2"> @Name</MudText>
                <MudText Typo="Typo.caption">@Email</MudText>


            </MudStack>

        </CardHeaderContent>
    </MudCardHeader>
</MudCard>

